                  <VBox fx:id="SongContainer" prefHeight="470.0" prefWidth="120.0" />
               </content>
            </ScrollPane>
         </children></AnchorPane>
      <ImageView fx:id="GenreImage" fitHeight="160.0" fitWidth="174.0" layoutX="228.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
