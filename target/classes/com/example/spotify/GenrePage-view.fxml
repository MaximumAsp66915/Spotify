            <Image url="@image/registerBackGround.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="500.0" fitWidth="209.0" layoutY="-1.0" pickOnBounds="true">
         <image>
            <Image url="@image/blackBackGround.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="-4.0" prefHeight="500.0" prefWidth="209.0">
         <children>
            <ScrollPane layoutX="36.0" layoutY="15.0" prefHeight="470.0" prefWidth="137.0">
